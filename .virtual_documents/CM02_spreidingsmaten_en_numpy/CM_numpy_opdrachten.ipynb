





import numpy as np





x = np.array([0, 1, 2, 3, 4, 5])
print(x)

# or
y = np.arange(0, 6)
print(y)





print(x.shape)

# or
print(x.ndim)





x = x.reshape(2, 3)
print(x)





x.shape





x.dtype





y = x.copy()
print(y)





x_transpose = np.transpose(x)
print(x_transpose)





y_transpose = np.transpose(y)
print(y_transpose)





y = y*2
print(y)





z = np.concatenate([x, y])
print(z)





from scipy import stats

# Average
z_average = np.average(z)

# Mode
z_mode = stats.mode(z, axis=None)

# Standard deviation
z_std = np.std(z)

print(z_average)
print(z_mode)
print(z_std)








